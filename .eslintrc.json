{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "eol-last": 0,
        "object-shorthand": 0,
        "padded-blocks": 0,
        "no-unused-vars": 0,
        "arrow-body-style": 0
    }
}