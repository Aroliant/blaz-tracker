{
  "name": "blaz",
  "version": "0.1.0",
  "private": true,
  "bin": "dist/server/index.js",
  "scripts": {
    "blaz:build": "node dev-scripts/build-blaz.js",
    "blaz:run": "node dev-scripts/run-blaz.js",
    "blaz:run:dev": "yarn update:env && yarn blaz:build --local && docker swarm leave -f ; yarn blaz:run",
    "blaz:start": "node app.bin",
    "client:build": "ng build",
    "client:lint": "ng lint",
    "client:test": "ng test",
    "db:clear": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "db:setup": "sequelize db:migrate && sequelize db:seed:all",
    "ng": "ng",
    "server:build": "babel server -d dist/server --copy-files",
    "server:lint": "eslint server",
    "server:pack": "pkg . -t node10.15.3-alpine-x64 --output app.bin",
    "server:pack:local": "pkg package.json --output app.bin",
    "server:start": "babel-node server/",
    "server:start:dev:api": "nodemon --exec babel-node server/index.js --api-dev",
    "server:start:dev": "nodemon --exec babel-node server/",
    "update:env": "sh dev-scripts/branch-to-env.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn client:lint && yarn server:lint",
      "pre-commit": "node dev-scripts/pre-commit.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "~8.2.1",
    "@angular/common": "~8.2.1",
    "@angular/compiler": "~8.2.1",
    "@angular/core": "~8.2.1",
    "@angular/forms": "~8.2.1",
    "@angular/platform-browser": "~8.2.1",
    "@angular/platform-browser-dynamic": "~8.2.1",
    "@angular/router": "~8.2.1",
    "@ng-select/ng-select": "^3.0.6",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "configstore": "^4.0.0",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "dockerode": "^2.5.8",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "figures": "^2.0.0",
    "fs-extra": "^7.0.1",
    "helmet": "^3.16.0",
    "http-proxy": "^1.17.0",
    "ipaddr.js": "^1.9.0",
    "is-valid-path": "^0.1.1",
    "js-base64": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "ngx-toastr": "^10.0.4",
    "nodemailer": "^6.0.0",
    "proxy-addr": "^2.0.4",
    "public-ip": "^3.1.0",
    "request": "^2.88.0",
    "require-from-string": "^2.0.2",
    "rxjs": "~6.5.2",
    "sequelize": "^5.3.1",
    "simple-git": "^1.110.0",
    "socket.io": "^2.2.0",
    "ssh2": "^0.8.2",
    "tar": "^4.4.8",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "~8.2.1",
    "@angular/language-service": "~8.2.1",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "babel-eslint": "8.2.2",
    "codelyzer": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^3.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "1.18.11",
    "pkg": "^4.4.0",
    "protractor": "~5.4.0",
    "semver": "^6.0.0",
    "sequelize-cli": "^5.4.0",
    "shelljs": "^0.8.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  },
  "pkg": {
    "scripts": [
      "dist/server/**/*.js",
      "dist/server/services/dns/**/*.js"
    ],
    "assets": [
      "dist/server/pages/**",
      "dist/server/env",
      "dist/server/template/**/*",
      "dist/dashboard/**/*"
    ]
  }
}
